# Auth0 Configuration
# Copy this file to .env and fill in your Auth0 credentials
# Note: Angular doesn't natively support .env files. These are reference values.
# Update the actual values in src/app/app.config.ts

# Auth0 Domain (e.g., dev-xyz123.us.auth0.com)
AUTH0_DOMAIN=your-auth0-domain.us.auth0.com

# Auth0 Client ID
AUTH0_CLIENT_ID=your-client-id

# Auth0 API Identifier / Audience (e.g., https://api.csediualumni.com)
AUTH0_AUDIENCE=https://api.csediualumni.com

# API Base URL
# Local development
API_BASE_URL_LOCAL=http://localhost:3000
# Production
API_BASE_URL_PRODUCTION=https://api.csediualumni.com
